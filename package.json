{
	"name": "@vis97c/vue-webpack-template",
	"version": "1.0.0",
	"description": "Template for a vue spa usign webpack",
	"main": "public_html/js/app.js",
	"author": "@vis97c",
	"scripts": {
		"production": "cross-env NODE_ENV=production webpack",
		"dev": "cross-env NODE_ENV=development webpack",
		"watch": "webpack-dev-server",
		"g_icons": "vsvg -s ./svg-icons -t ./src/js/compiled-icons"
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-proposal-json-strings": "^7.7.4",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-syntax-import-meta": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.0.5",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"eslint": "^6.8.0",
		"eslint-plugin-prettier": "^3.1.2",
		"html-beautify-webpack-plugin": "^1.0.5",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^4.2.1",
		"lint-staged": "^10.0.2",
		"mini-css-extract-plugin": "^0.6.0",
		"node-sass": "^4.13.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-custom-properties": "^9.0.2",
		"postcss-loader": "^3.0.0",
		"prettier": "1.19.1",
		"prettier-stylelint": "^0.4.2",
		"purgecss-webpack-plugin": "^1.6.0",
		"sass-loader": "^8.0.0",
		"stylelint": "^13.0.0",
		"stylelint-config-prettier": "^8.0.1",
		"stylelint-prettier": "^1.1.2",
		"style-loader": "^1.1.2",
		"terser-webpack-plugin": "^2.2.2",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.0",
		"webpack-dev-server": "^3.3.1",
		"webpack-notifier": "^1.8.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.7.0",
		"core-js": "^3.4.8",
		"dotenv": "^8.2.0",
		"es6-promise": "^4.2.8",
		"jquery": "^3.4.1"
	},
	"resolutions": {
		"webpack-notifier/node-notifier": "6.0.0"
	},
	"browserslist": [
		"defaults",
		"not ie < 11",
		"last 2 versions",
		"> 1%",
		"iOS 7",
		"last 3 iOS versions"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.css": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.scss": [
			"stylelint --syntax=scss --fix",
			"prettier --write"
		]
	},
	"license": "MIT"
}
